{
    "name": "evolver/repository-manager-module",
    "description": "Manage (Doctrine) Repositories via Zend Framework's ServiceManager",
    "license": "proprietary",
    "repositories": [
        {
            "packagist": false
        },
        {
            "type": "composer",
            "url": "http://satis.evolver.de"
        },
        {
            "type": "composer",
            "url": "http://satis.evolver.de/cache"
        }
    ],
    "require": {
        "php": "~5.5",
        "doctrine/common": "~2.5",
        "zendframework/zend-modulemanager": "~2.4",
        "zendframework/zend-servicemanager": "~2.4"
    },
    "require-dev": {
        "doctrine/doctrine-orm-module": "~0.8",
        "phpunit/phpunit": "~4.6",
        "zendframework/zend-i18n": "~2.4",
        "zendframework/zend-inputfilter": "~2.4",
        "zendframework/zend-log": "~2.4",
        "zendframework/zend-filter": "~2.4",
        "zendframework/zend-form": "~2.4",
        "zendframework/zend-serializer": "~2.4",
        "zendframework/zend-stdlib": "~2.4",
        "zendframework/zend-test": "~2.4",
        "zendframework/zend-validator": "~2.4"
    },
    "autoload": {
        "psr-4": {
            "Evolver\\RepositoryManagerModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Evolver\\RepositoryManagerModuleTest\\": "tests/src"
        }
    },
    "authors": [
        {
            "name": "Michael Kühn",
            "email": "michael.kuehn@evolver.de",
            "role": "Developer"
        },
        {
            "name": "Daniel Schröder",
            "email": "daniel.schroeder@evolver.de",
            "role": "Developer"
        }
    ]
}
